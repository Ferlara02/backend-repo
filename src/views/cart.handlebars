<main>
    <h1>Lista de Carrito</h1>
    {{#if cartRes}}
        <h2>Cart is {{cartRes}}</h2>
    {{/if}}
    <h2>Cart ID: {{idCart}}</h2>
    <div class="listaContainer">
        {{#if cartProds.length}}
            {{#each cartProds}}
                <ul class="productoContainer">
                    <li>{{title}}</li>
                    <li>Qty: {{quantity}}</li>
                    <li>Price {{total}}</li>
                </ul>
            {{/each}}
            {{else}}
                <p>No hay productos</p>
        {{/if}}
    </div>
</main>
